    "                clean_fda.append(i)\n",
    "                clean_ndc.append(j)\n",
    "        #match = match.append({'company matches': matches}, ignore_index = True)\n",
    "    match['clean fda'] = clean_fda\n",
    "    match['clean ndc'] = clean_ndc\n",
    "    match['fuzz ratio'] = score\n",
    "    return match"
